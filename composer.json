{
    "name": "blaisebueno/laravel-repository",
    "description": "A Laravel package implementing a clean, scalable repository design pattern architecture to abstract data persistence and promote testable, maintainable code.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "John Blaise Bueno",
            "email": "blaisebueno@gmail.com"
        }
    ],
    "keywords": [
        "laravel",
        "repository",
        "design pattern",
        "repository pattern",
        "data abstraction",
        "service layer",
        "php"
    ],
    "require": {
        "php": "^7.4|^8.0"
    },
    "autoload": {
        "psr-4": {
            "BlaiseBueno\\LaravelRepository\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BlaiseBueno\\LaravelRepository\\LaravelRepositoryServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
